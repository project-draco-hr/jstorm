{
  long start=System.nanoTime();
  try {
    event=new LogPage.Event(paramMap);
    conf=getNimbusConf();
    queryLog(event);
  }
 catch (  Exception e) {
    LOG.error(e.getMessage(),e);
    throw e;
  }
 finally {
    long end=System.nanoTime();
    UIMetrics.updateHistorgram(this.getClass().getSimpleName(),(end - start) / 1000000.0d);
  }
}
