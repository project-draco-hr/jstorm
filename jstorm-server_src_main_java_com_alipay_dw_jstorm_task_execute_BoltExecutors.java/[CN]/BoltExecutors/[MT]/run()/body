{
  while (true) {
    Tuple tuple=null;
    try {
      tuple=recv_tuple_queue.take();
    }
 catch (    InterruptedException e1) {
      return;
    }
    if (tuple == null) {
      return;
    }
    tuple_start_times.put(tuple,System.currentTimeMillis());
    try {
      bolt.execute(tuple);
    }
 catch (    Exception e) {
      error=e;
      LOG.error("bolt execute error ",e);
      report_error.report(e);
    }
  }
}
