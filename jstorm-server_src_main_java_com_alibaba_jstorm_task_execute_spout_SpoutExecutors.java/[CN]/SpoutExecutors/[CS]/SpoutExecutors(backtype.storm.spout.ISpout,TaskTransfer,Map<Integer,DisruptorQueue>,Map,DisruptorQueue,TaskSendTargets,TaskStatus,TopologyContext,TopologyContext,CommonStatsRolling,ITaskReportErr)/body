{
  super(_transfer_fn,_storm_conf,_puller,innerTaskTransfer,topology_context,_user_context,_task_stats,taskStatus,_report_error);
  this.spout=_spout;
  this.max_spout_pending=JStormUtils.parseInt(storm_conf.get(Config.TOPOLOGY_MAX_SPOUT_PENDING));
  this.nextTupleTimer=Metrics.registerTimer(idStr,MetricDef.EXECUTE_TIME,String.valueOf(taskId),Metrics.MetricType.TASK);
  this.ackerTimer=Metrics.registerTimer(idStr,MetricDef.ACKER_TIME,String.valueOf(taskId),Metrics.MetricType.TASK);
  this.emptyCpuCounter=new TimerRatio();
  Metrics.register(idStr,MetricDef.EMPTY_CPU_RATIO,emptyCpuCounter,String.valueOf(taskId),Metrics.MetricType.TASK);
  isSpoutFullSleep=ConfigExtension.isSpoutPendFullSleep(storm_conf);
  LOG.info("isSpoutFullSleep:" + isSpoutFullSleep);
}
