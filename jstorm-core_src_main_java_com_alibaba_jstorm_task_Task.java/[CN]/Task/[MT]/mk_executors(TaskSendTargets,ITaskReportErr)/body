{
  if (taskObj instanceof IBolt) {
    return new BoltExecutors(this,(IBolt)taskObj,taskTransfer,innerTaskTransfer,stormConf,sendTargets,taskStatus,topologyContext,userContext,taskStats,report_error);
  }
 else   if (taskObj instanceof ISpout) {
    if (isSingleThread(stormConf) == true) {
      return new SingleThreadSpoutExecutors(this,(ISpout)taskObj,taskTransfer,innerTaskTransfer,stormConf,sendTargets,taskStatus,topologyContext,userContext,taskStats,report_error);
    }
 else {
      return new MultipleThreadSpoutExecutors(this,(ISpout)taskObj,taskTransfer,innerTaskTransfer,stormConf,sendTargets,taskStatus,topologyContext,userContext,taskStats,report_error);
    }
  }
  return null;
}
