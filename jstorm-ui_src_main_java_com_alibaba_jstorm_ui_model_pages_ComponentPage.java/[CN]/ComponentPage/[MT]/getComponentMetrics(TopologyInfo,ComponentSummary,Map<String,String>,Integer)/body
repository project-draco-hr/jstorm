{
  MetricInfo metricInfo=topologyInfo.get_metrics().get_componentMetric().get(componentSummary.get_name());
  if (metricInfo == null) {
    LOG.info("No component metric  of " + componentSummary.get_name());
    return null;
  }
  TableData table=UIUtils.getMetricTable(metricInfo,window);
  table.setName(componentSummary.get_name() + "-Metrics-" + getWindowStr(window));
  return table;
}
