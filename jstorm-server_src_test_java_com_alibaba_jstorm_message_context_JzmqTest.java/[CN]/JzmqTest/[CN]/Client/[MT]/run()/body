{
  isRunning=true;
  System.out.println("Start client thread ");
  s.connect("tcp://127.0.0.1:6669");
  byte[] recvBytes;
  while (isRunning) {
    try {
      recvBytes=s.recv(0);
      if (recvBytes != null) {
        System.out.println("recv: " + new String(recvBytes));
      }
    }
 catch (    Exception e) {
      break;
    }
  }
  s.close();
  System.out.println("Stop client thread ");
}
