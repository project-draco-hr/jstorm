{
  long defaultSize=JStromServerConfigExtension.getMemSizePerWorker(conf);
  int defaultCpu=JStromServerConfigExtension.getCpuSlotPerWorker(conf);
  for (  ResourceWorkerSlot worker : result) {
    if (worker.getMemSize() <= 0)     worker.setMemSize(defaultSize);
    if (worker.getCpu() <= 0)     worker.setCpu(defaultCpu);
  }
}
