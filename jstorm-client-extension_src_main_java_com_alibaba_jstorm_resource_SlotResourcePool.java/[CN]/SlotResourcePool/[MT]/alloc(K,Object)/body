{
  if (slot == null) {
    return null;
  }
synchronized (type) {
    Boolean value=slots.get(slot);
    if (value == null) {
      LOG.info(type + " no this slot " + slot);
      return null;
    }
    if (value == true) {
      LOG.debug(type + " slot " + slot+ " is assigned");
      return null;
    }
    slots.put(slot,Boolean.valueOf(true));
    LOG.debug(type + " assign " + slot+ " to "+ context);
    return slot;
  }
}
