{
  if (event == null) {
    return;
  }
  timer.start();
  try {
    TupleExt tuple=(TupleExt)event;
    int taskid=tuple.getTargetTaskId();
    byte[] tupleMessage=serializer.serialize(tuple);
    TaskMessage taskMessage=new TaskMessage(taskid,tupleMessage);
    transferQueue.publish(taskMessage);
  }
  finally {
    timer.stop();
  }
}
