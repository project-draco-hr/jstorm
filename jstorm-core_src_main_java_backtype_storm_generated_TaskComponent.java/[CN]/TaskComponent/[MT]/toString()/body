{
  StringBuilder sb=new StringBuilder("TaskComponent(");
  boolean first=true;
  sb.append("taskId:");
  sb.append(this.taskId);
  first=false;
  if (!first)   sb.append(", ");
  sb.append("component:");
  if (this.component == null) {
    sb.append("null");
  }
 else {
    sb.append(this.component);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
