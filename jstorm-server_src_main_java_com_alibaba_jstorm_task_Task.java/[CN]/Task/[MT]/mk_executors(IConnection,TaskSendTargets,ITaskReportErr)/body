{
  if (taskObj instanceof IBolt) {
    return new BoltExecutors((IBolt)taskObj,taskTransfer,innerTaskTransfer,stormConf,_puller,sendTargets,taskStatus,topologyContext,userContext,taskStats,_report_error);
  }
 else   if (taskObj instanceof ISpout) {
    return new SpoutExecutors((ISpout)taskObj,taskTransfer,innerTaskTransfer,stormConf,_puller,sendTargets,taskStatus,topologyContext,userContext,taskStats,_report_error);
  }
  return null;
}
