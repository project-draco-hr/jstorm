{
  if (taskObj instanceof IBolt) {
    return new BoltExecutors((IBolt)taskObj,taskTransfer,innerTaskTransfer,stormConf,deserializeQueue,sendTargets,taskStatus,topologyContext,userContext,taskStats,report_error);
  }
 else   if (taskObj instanceof ISpout) {
    if (isSingleThread(stormConf) == true) {
      return new SingleThreadSpoutExecutors((ISpout)taskObj,taskTransfer,innerTaskTransfer,stormConf,deserializeQueue,sendTargets,taskStatus,topologyContext,userContext,taskStats,report_error);
    }
 else {
      return new MultipleThreadSpoutExecutors((ISpout)taskObj,taskTransfer,innerTaskTransfer,stormConf,deserializeQueue,sendTargets,taskStatus,topologyContext,userContext,taskStats,report_error);
    }
  }
  return null;
}
