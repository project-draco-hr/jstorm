{
  int queueSize=JStormUtils.parseInt(stormConf.get(Config.TOPOLOGY_EXECUTOR_RECEIVE_BUFFER_SIZE),256);
  WaitStrategy waitStrategy=(WaitStrategy)Utils.newInstance((String)stormConf.get(Config.TOPOLOGY_DISRUPTOR_WAIT_STRATEGY));
  DisruptorQueue queue=new DisruptorQueue(new SingleThreadedClaimStrategy(queueSize),waitStrategy);
  deserializeQueues.put(taskid,queue);
  return queue;
}
