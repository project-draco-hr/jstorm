{
  final Snapshot snapshot=histogram.getSnapshot();
  logger.info(marker,"type=HISTOGRAM, name={}, count={}, min={}, max={}, mean={}, stddev={}, " + "median={}, p75={}, p95={}, p98={}, p99={}, p999={}",name,histogram.getCount(),snapshot.getMin(),snapshot.getMax(),snapshot.getMean(),snapshot.getStdDev(),snapshot.getMedian(),snapshot.get75thPercentile(),snapshot.get95thPercentile(),snapshot.get98thPercentile(),snapshot.get99thPercentile(),snapshot.get999thPercentile());
}
