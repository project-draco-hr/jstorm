{
  if (p1 == null || p2 == null) {
    return null;
  }
 else {
    int p1Szie=p1.size();
    int p2Size=p2.size();
    if (p1Szie != p1Szie) {
      return null;
    }
 else {
      List<Double> rtn=new ArrayList<Double>();
      for (int i=0; i < p1Szie; i++) {
        rtn.set(i,p1.get(i) + p2.get(i));
      }
      return rtn;
    }
  }
}
