{
  FacesContext ctx=FacesContext.getCurrentInstance();
  if (ctx.getExternalContext().getRequestParameterMap().get(HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_POS) != null) {
    position=ctx.getExternalContext().getRequestParameterMap().get(HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_POS);
  }
  init();
}
