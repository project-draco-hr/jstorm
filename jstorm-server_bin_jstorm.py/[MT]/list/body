def list(*args):
    'Syntax: [jstorm list]\n\n    List cluster information\n    '
    childopts = (' -Dstorm.root.logger=INFO,stdout -Dlog4j.configuration=File:%s/conf/aloha_log4j.properties' % JSTORM_DIR)
    exec_storm_class('backtype.storm.command.list', args=args, jvmtype='-client -Xms256m -Xmx256m', extrajars=[CONF_DIR, (JSTORM_DIR + '/bin'), LOG4J_CONF], childopts=childopts)
