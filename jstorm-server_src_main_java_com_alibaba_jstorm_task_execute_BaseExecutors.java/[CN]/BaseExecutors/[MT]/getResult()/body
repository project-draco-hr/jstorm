{
  if (taskStatus.isRun()) {
    return 0;
  }
 else   if (taskStatus.isPause()) {
    return 0;
  }
 else   if (taskStatus.isShutdown()) {
    this.shutdown();
    return -1;
  }
 else {
    LOG.info("Unknow TaskStatus, shutdown executing thread of " + idStr);
    this.shutdown();
    return -1;
  }
}
