{
  final String PROXY_URL="http://%s:%s/logview?%s=%s&%s=%s";
  String url=String.format(PROXY_URL,NetWorkUtils.host2Ip(event.host),event.port,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_CMD,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_CMD_LIST,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_DIR,event.dir);
  HttpClient client=HttpClientBuilder.create().build();
  HttpPost post=new HttpPost(url);
  HttpResponse response=client.execute(post);
  if (response.getStatusLine().getStatusCode() == 200) {
    String data=EntityUtils.toString(response.getEntity());
    return parseString(data,event);
  }
 else {
    String data=EntityUtils.toString(response.getEntity());
    List<TableData> ret=new ArrayList<TableData>();
    ret.add(UIUtils.errorTable("Failed to list dir"));
    return ret;
  }
}
