{
switch (field) {
case ID:
    return get_id();
case NAME:
  return get_name();
case STATUS:
return get_status();
case UPTIME_SECS:
return Integer.valueOf(get_uptimeSecs());
case NUM_TASKS:
return Integer.valueOf(get_numTasks());
case NUM_WORKERS:
return Integer.valueOf(get_numWorkers());
case ERROR_INFO:
return get_errorInfo();
}
throw new IllegalStateException();
}
