{
  Object obj=null;
  try {
    if ((flags & 0x01) == 0x01) {
      obj=disruptorQueue.poll();
    }
 else {
      obj=disruptorQueue.take();
    }
  }
 catch (  Exception e) {
    LOG.warn("Occur disruptor excpetion ",e);
    return null;
  }
  if (obj != null) {
    return (byte[])obj;
  }
 else {
    return null;
  }
}
