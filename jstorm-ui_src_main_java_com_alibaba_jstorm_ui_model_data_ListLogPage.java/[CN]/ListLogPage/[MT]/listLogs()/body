{
  String url=String.format(PROXY_URL,host,port,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_CMD,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_CMD_LIST,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_DIR,parent);
  try {
    HttpClient client=HttpClientBuilder.create().build();
    HttpPost post=new HttpPost(url);
    HttpResponse response=client.execute(post);
    if (response.getStatusLine().getStatusCode() == 200) {
      String data=EntityUtils.toString(response.getEntity());
      parseString(data);
    }
 else {
      String data=EntityUtils.toString(response.getEntity());
      summary=("Failed to get files\n" + data);
    }
  }
 catch (  Exception e) {
    summary=("Failed to get files\n" + e.getMessage());
    LOG.error(e.getCause(),e);
  }
}
