{
  try {
    if (supportRecvThread == true) {
      disruptorRecvQueue.consumeBatchWhenAvailable(this);
    }
 else {
      disruptorRecvQueue.consumeBatch(this);
    }
  }
 catch (  Exception e) {
    if (taskStatus.isShutdown() == false) {
      LOG.error("Actor occur unknow exception ",e);
    }
  }
}
