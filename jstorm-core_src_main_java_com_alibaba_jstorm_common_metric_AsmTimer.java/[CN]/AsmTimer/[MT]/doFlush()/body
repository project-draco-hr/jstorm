{
  long[] values=unFlushed.getSnapshot().getValues();
  for (  Timer timer : timerMap.values()) {
    for (    long val : values) {
      timer.update(val,TimeUnit.MILLISECONDS);
      for (      AsmMetric metric : this.assocMetrics) {
        metric.updateDirectly(val);
      }
    }
  }
  this.unFlushed=newTimer();
}
