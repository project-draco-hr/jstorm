{
  if (supervisor != null) {
    supervisor.ShutdownAllWorkers();
    supervisor.shutdown();
  }
  if (nimbusServer != null) {
    nimbusServer.cleanup();
  }
  if (zookeeper != null)   zookeeper.shutdown();
  if (tmpDir != null) {
    for (    String dir : tmpDir) {
      try {
        PathUtils.rmr(dir);
      }
 catch (      IOException e) {
        LOG.error("fail to delete " + dir,e);
      }
    }
  }
}
