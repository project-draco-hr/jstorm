{
  Metric metric=null;
  if (type == 0) {
    Counter<Double> counter=new Counter<Double>(0.0d);
    metric=counter;
  }
 else   if (type == 1) {
    metric=new Meter();
  }
 else   if (type == 2) {
    metric=new Histogram();
  }
 else {
    return null;
  }
  metric.setWindowSeconds(windows);
  metric.init();
  return metric;
}
