{
  if (isFinishInit == false) {
    initWrapper();
  }
  while (taskStatus.isShutdown() == false) {
    try {
      exeQueue.consumeBatchWhenAvailable(this);
      processControlEvent();
    }
 catch (    Throwable e) {
      if (taskStatus.isShutdown() == false) {
        LOG.error(idStr + " bolt exeutor  error",e);
      }
    }
  }
}
