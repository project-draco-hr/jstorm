{
  if (!isFinishInit) {
    initWrapper();
  }
  while (!taskStatus.isShutdown()) {
    try {
      exeQueue.consumeBatchWhenAvailable(this);
      processControlEvent();
    }
 catch (    Throwable e) {
      if (!taskStatus.isShutdown()) {
        LOG.error(idStr + " bolt exeutor  error",e);
      }
    }
  }
}
