{
switch (field) {
case TASK_ID:
    return Integer.valueOf(get_task_id());
case COMPONENT_ID:
  return get_component_id();
case GAUGE:
return get_gauge();
case COUNTER:
return get_counter();
case METER:
return get_meter();
case TIMER:
return get_timer();
case HISTOGRAM:
return get_histogram();
}
throw new IllegalStateException();
}
