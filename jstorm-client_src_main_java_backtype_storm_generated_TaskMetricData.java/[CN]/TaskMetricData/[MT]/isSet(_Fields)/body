{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case TASK_ID:
    return is_set_task_id();
case COMPONENT_ID:
  return is_set_component_id();
case GAUGE:
return is_set_gauge();
case COUNTER:
return is_set_counter();
case METER:
return is_set_meter();
case TIMER:
return is_set_timer();
case HISTOGRAM:
return is_set_histogram();
}
throw new IllegalStateException();
}
