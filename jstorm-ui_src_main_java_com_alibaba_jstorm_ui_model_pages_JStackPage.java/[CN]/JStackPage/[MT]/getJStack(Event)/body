{
  final String PROXY_URL="http://%s:%s/logview?%s=%s&%s=%s";
  String baseUrl=String.format(PROXY_URL,NetWorkUtils.host2Ip(event.host),event.logServerPort,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_CMD,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_CMD_JSTACK,HttpserverUtils.HTTPSERVER_LOGVIEW_PARAM_WORKER_PORT,event.workerport);
  String url=baseUrl;
  HttpClient client=HttpClientBuilder.create().build();
  HttpPost post=new HttpPost(url);
  HttpResponse response=client.execute(post);
  String errMsg=getErrMsg(event);
  if (response.getStatusLine().getStatusCode() == 200) {
    String data=EntityUtils.toString(response.getEntity());
    if (StringUtils.isBlank(data)) {
      throw new IllegalArgumentException(errMsg);
    }
    return data;
  }
 else {
    throw new IllegalArgumentException(errMsg);
  }
}
