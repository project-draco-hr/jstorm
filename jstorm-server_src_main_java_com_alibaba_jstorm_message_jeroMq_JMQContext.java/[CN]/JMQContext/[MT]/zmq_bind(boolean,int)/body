{
  String url=null;
  if (distributeZmq) {
    if (ipc) {
      url="ipc://" + port + ".ipc";
    }
 else {
      url="tcp://*:" + port;
    }
  }
 else {
    url="inproc://" + port;
  }
  Socket socket=context.socket(ZMQ.PULL);
  socket.connect(url);
  socket.setHWM(maxQueueMsg);
  LOG.info("Create jzmq receiver {}",url);
  return new JZMQRecvConnection(socket);
}
