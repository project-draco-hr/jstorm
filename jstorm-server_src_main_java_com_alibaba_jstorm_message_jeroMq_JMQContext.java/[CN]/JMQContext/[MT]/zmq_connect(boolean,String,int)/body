{
  String url=null;
  if (distributeZmq) {
    if (ipc) {
      url="ipc://" + port + ".ipc";
    }
 else {
      url="tcp://" + host + ":"+ port;
    }
  }
 else {
    url="inproc://" + port;
  }
  Socket socket=context.socket(ZMQ.PUSH);
  socket.bind(url);
  socket.setLinger(linger_ms);
  socket.setHWM(maxQueueMsg);
  LOG.info("Create jzmq sender {}",url);
  return new JZMQSendConnection(socket);
}
