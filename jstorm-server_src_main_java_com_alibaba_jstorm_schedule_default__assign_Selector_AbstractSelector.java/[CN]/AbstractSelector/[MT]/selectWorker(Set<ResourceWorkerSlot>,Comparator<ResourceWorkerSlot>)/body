{
  Set<ResourceWorkerSlot> result=new HashSet<ResourceWorkerSlot>();
  ResourceWorkerSlot best=null;
  for (  ResourceWorkerSlot worker : list) {
    if (best == null) {
      best=worker;
      result.add(worker);
      continue;
    }
    if (c.compare(best,worker) == 0) {
      result.add(worker);
    }
 else     if (c.compare(best,worker) > 0) {
      best=worker;
      result.clear();
      result.add(best);
    }
  }
  return result;
}
