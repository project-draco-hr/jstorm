{
  if (event == null) {
    return;
  }
  Tuple tuple=(Tuple)event;
  task_stats.recv_tuple(tuple.getSourceComponent(),tuple.getSourceStreamId());
  tuple_start_times.put(tuple,System.currentTimeMillis());
  try {
    bolt.execute(tuple);
  }
 catch (  Exception e) {
    error=e;
    LOG.error("bolt execute error ",e);
    report_error.report(e);
  }
}
