{
  ByteBuffer buff=ByteBuffer.wrap(packet);
  int port=buff.getInt();
  byte[] message=null;
  if (KryoTupleDeserializer.USE_RAW_PACKET == true) {
    message=packet;
  }
 else {
    message=new byte[buff.array().length - (Integer.SIZE / 8)];
    buff.get(message);
  }
  PacketPair pair=new PacketPair(port,message);
  return pair;
}
