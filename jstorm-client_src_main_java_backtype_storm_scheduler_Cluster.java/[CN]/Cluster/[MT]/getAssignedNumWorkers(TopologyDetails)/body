{
  if (topology == null) {
    return 0;
  }
  SchedulerAssignment assignment=this.getAssignmentById(topology.getId());
  if (assignment == null) {
    return 0;
  }
  Set<WorkerSlot> slots=new HashSet<WorkerSlot>();
  slots.addAll(assignment.getExecutorToSlot().values());
  return slots.size();
}
