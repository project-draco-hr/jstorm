{
  if (clusterConfig.containsKey(name)) {
    Integer port=clusterConfig.get(name).getSupervisorPort();
    if (port == null) {
      flushClusterConfig();
      port=clusterConfig.get(name).getSupervisorPort();
    }
    return port;
  }
  LOG.info("can not find the cluster with name:" + name);
  return null;
}
