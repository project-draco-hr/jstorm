{
  Class<?> result=null;
  try {
    result=this.findLoadedClass(name);
    if (result != null) {
      return result;
    }
    if (name.startsWith("org.slf4j") == false && name.startsWith("org.apache.log4j") == false && name.startsWith("ch.qos.logback") == false) {
      result=findClass(name);
      if (result != null) {
        return result;
      }
    }
  }
 catch (  Exception e) {
  }
  return parent.loadClass(name);
}
