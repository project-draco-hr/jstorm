{
  LOG.info("Successfully start ZMQ Recv thread");
  while (isClosed() == false) {
    try {
      TaskMessage message=recv(0);
      enqueue(message);
    }
 catch (    Exception e) {
      LOG.warn("ZMQ Recv thread receive error",e);
    }
  }
  LOG.info("Successfully shutdown ZMQ Recv thread");
}
