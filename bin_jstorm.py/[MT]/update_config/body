def update_config(*args):
    'Syntax: [jstorm restart topology-name [conf]]\n    '
    childopts = get_client_childopts()
    exec_storm_class('backtype.storm.command.update_config', args=args, jvmtype='-client -Xms256m -Xmx256m', extrajars=[JSTORM_CONF_DIR, (JSTORM_DIR + '/bin'), CLIENT_CONF_FILE], childopts=childopts)
