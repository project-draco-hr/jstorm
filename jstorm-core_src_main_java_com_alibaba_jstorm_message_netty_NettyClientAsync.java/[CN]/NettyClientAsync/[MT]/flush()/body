{
  if (isClosed() == true) {
    return;
  }
  if (flush_later.get() == false) {
    return;
  }
  Channel channel=isChannelReady();
  if (channel == null) {
    return;
  }
  flush_later.set(false);
  MessageBatch toBeFlushed=messageBatchRef.getAndSet(null);
  flushRequest(channel,toBeFlushed);
}
